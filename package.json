		"joi": "17.13.3",
		"mochawesome": "^7.1.4",
		"mongoose": "8.5.3",
		"supertest": "^7.1.4",
		"winston": "3.14.1",
		"winston-mongodb": "5.1.1"
	},
	"devDependencies": {
		"@types/chai": "5.0.1",
		"@types/express": "4.17.21",
		"@types/mocha": "10.0.10",
		"@types/node": "22.3.0",
		"@types/sinon": "17.0.3",
		"@types/supertest": "^6.0.3",
		"chai": "4.5.0",
		"chai-http": "4.4.0",
		"mocha": "11.1.0",
		"mongodb-memory-server": "10.1.3",
		"nyc": "^17.1.0",
